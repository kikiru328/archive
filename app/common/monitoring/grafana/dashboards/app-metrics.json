{
  "dashboard": {
    "id": null,
    "title": "Application Metrics",
    "tags": ["fastapi", "application"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Users",
        "type": "stat",
        "targets": [
          {
            "expr": "total_users",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "User Registrations Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(user_registrations_total[5m])",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "blue",
                  "value": null
                }
              ]
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 3,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "orange",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "User Metrics Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "total_users",
            "refId": "A",
            "legendFormat": "Total Users"
          },
          {
            "expr": "active_users",
            "refId": "B",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Registration Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(user_registrations_total[1m])",
            "refId": "A",
            "legendFormat": "Registrations/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        }
      }
    ]
  }
}
